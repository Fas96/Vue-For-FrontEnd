ader/dist/cjs.js","mtime":1655450375361},{"path":"/Users/fasbhim/Documents/2022/work/2022_studyMaterial/vue-js/vue-js/doc/downloads/13.routing/routing-01-starting-setup/node_modules/vue-loader-v16/dist/index.js","mtime":1655450376039}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"mappings":";;;;;;+CAWgC;;;;;;;uBAV9BA,oBAWU,SAXV,EAWU,IAXV,EAWU,CAVRC,oBAAuB,IAAvB,EAAuB,IAAvB,EAAuBC,iBAAhBC,cAAgB,CAAvB,EAAe;EAAA;EAAf,CAUQ,EATRF,oBAOK,IAPL,EAOK,IAPL,EAOK,oBANHD,oBAKaI,SALb,EAKa,IALb,EAKaC,YAJMF,aAIN,EAJa,UAAjBG,MAAiB,EAAX;yBADfC,aAKaC,oBALb,EAKa;MAHVC,GAAG,EAAEH,MAAM,CAACI,EAGF;MAFVC,IAAI,EAAEL,MAAM,CAACM,QAEH;MADVC,IAAI,EAAEP,MAAM,CAACO;IACH,CALb;;IAAA;GAKa,CALb;;EAAA,CAMG,EAPL,CASQ,EADRC,aAAsDC,sBAAtD,EAAsD;IAAzCC,EAAE,EAAC;EAAsC,CAAtD,EAA2B;sBAAC;MAAA,OAAY,YAAZ;IAAA,EAAD;;;;EAAA,CAA3B,CACQ,CAXV","names":["_createElementBlock","_createElementVNode","_toDisplayString","$data","_Fragment","_renderList","member","_createBlock","_component_user_item","key","id","name","fullName","role","_createVNode","_component_router_link","to"],"sourceRoot":"","sources":["/Users/fasbhim/Documents/2022/work/2022_studyMaterial/vue-js/vue-js/doc/downloads/13.routing/routing-01-starting-setup/src/components/teams/TeamMembers.vue"],"sourcesContent":["<template>\n  <section>\n    <h2>{{ teamName }}</h2>\n    <ul>\n      <user-item\n        v-for=\"member in members\"\n        :key=\"member.id\"\n        :name=\"member.fullName\"\n        :role=\"member.role\"\n      ></user-item>\n    </ul>\n    <router-link to=\"/teams/t2\">Go to team-2</router-link>\n  </section>\n</template>\n\n<script>\nimport UserItem from '../users/UserItem.vue';\n\nexport default {\n  inject: ['users','teams'],\n  components: {\n    UserItem\n  },\n  computed: {\n    availableTeamRoutes() {\n      const teamRoutes = [];\n\n      this.teams.forEach((team) => teamRoutes.push(team.id));\n\n      return teamRoutes;\n    },\n  },\n  data() {\n    return {\n      teamName: 'Test',\n      members: [\n        { id: 'u1', fullName: 'Fas Testing', role: 'Engineer' },\n        { id: 'u2', fullName: 'Fas Basics', role: 'Engineer' },\n      ],\n    };\n  },\n  methods:{\n    loadTeamMembers(route){\n      //we can get access to injected data\n      const teamId=route.params.teamId\n      if (!this.availableTeamRoutes.includes(teamId)) return;\n      const selectedTeam=this.teams.find(team=>team.id===teamId);\n      if (!selectedTeam) {\n        return;\n      }\n      const members= selectedTeam.members;\n      const selectedMembers=[]\n      for (const member of members) {\n        const thisUser=this.users.find(user=>user.id===member)\n        selectedMembers.push(thisUser)\n      }\n      this.members=selectedMembers;\n      this.teamName=selectedTeam.name;\n    }\n  },\n\n  created() {\n    this.loadTeamMembers(this.$route)\n  },\n  watch:{\n    $route(newRoute){\n      this.loadTeamMembers(newRoute)\n    }\n  }\n};\n</script>\n\n<style scoped>\nsection {\n  margin: 2rem auto;\n  max-width: 40rem;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\n  padding: 1rem;\n  border-radius: 12px;\n}\n\nh2 {\n  margin: 0.5rem 0;\n}\n\nul {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n</style>"]}]}