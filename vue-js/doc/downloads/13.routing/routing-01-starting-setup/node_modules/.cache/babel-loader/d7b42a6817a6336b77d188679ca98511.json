{{ teamName }}</h2>\n    <ul>\n      <user-item\n        v-for=\"member in members\"\n        :key=\"member.id\"\n        :name=\"member.fullName\"\n        :role=\"member.role\"\n      ></user-item>\n    </ul>\n    <router-link to=\"/teams/t2\">Go to team-2</router-link>\n  </section>\n</template>\n\n<script>\nimport UserItem from '../users/UserItem.vue';\n\nexport default {\n  inject: ['users','teams'],\n  components: {\n    UserItem\n  },\n  computed: {\n    availableTeamRoutes() {\n      const teamRoutes = [];\n\n      this.teams.forEach((team) => teamRoutes.push(team.id));\n\n      return teamRoutes;\n    },\n  },\n  data() {\n    return {\n      teamName: 'Test',\n      members: [\n        { id: 'u1', fullName: 'Fas Testing', role: 'Engineer' },\n        { id: 'u2', fullName: 'Fas Basics', role: 'Engineer' },\n      ],\n    };\n  },\n  methods:{\n    loadTeamMembers(route){\n      //we can get access to injected data\n      const teamId=route.params.teamId\n      if (!this.availableTeamRoutes.includes(teamId)) return;\n      const selectedTeam=this.teams.find(team=>team.id===teamId);\n      if (!selectedTeam) {\n        return;\n      }\n      const members= selectedTeam.members;\n      const selectedMembers=[]\n      for (const member of members) {\n        const thisUser=this.users.find(user=>user.id===member)\n        selectedMembers.push(thisUser)\n      }\n      this.members=selectedMembers;\n      this.teamName=selectedTeam.name;\n    }\n  },\n\n  created() {\n    this.loadTeamMembers(this.$route)\n  },\n  watch:{\n    $route(newRoute){\n      this.loadTeamMembers(newRoute)\n    }\n  }\n};\n</script>\n\n<style scoped>\nsection {\n  margin: 2rem auto;\n  max-width: 40rem;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\n  padding: 1rem;\n  border-radius: 12px;\n}\n\nh2 {\n  margin: 0.5rem 0;\n}\n\nul {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n</style>"]}]}