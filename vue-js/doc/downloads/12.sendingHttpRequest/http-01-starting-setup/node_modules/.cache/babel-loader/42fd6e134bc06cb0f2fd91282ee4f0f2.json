ersion":3,"mappings":";AAwBA,OAAOA,YAAP,MAAyB,oBAAzB;AAEA,eAAe;EACb;EACAC,UAAU,EAAE;IACVD,YAAY,EAAZA;EADU,CAFC;EAKbE,IALa,kBAKP;IACJ,OAAO;MACLC,OAAO,EAAC,EADH;MAELC,SAAS,EAAE,KAFN;MAGLC,KAAK,EAAC;IAHD,CAAP;EAKD,CAXY;EAYbC,OAZa,qBAYH;IACR,KAAKC,OAAL;EACD,CAdY;EAebC,OAAO,EAAC;IACND,OADM,qBACG;MAAA;;MACP,KAAKH,SAAL,GAAe,IAAf;MACA,KAAKC,KAAL,GAAW,IAAX;MACAI,KAAK,CAAC,iEAAD,CAAL,CACKC,IADL,CACU,UAACC,QAAD,EAAa;QAAE,IAAGA,QAAQ,CAACC,EAAZ,EAAe;UAAE,OAAOD,QAAQ,CAACE,IAAT,EAAP;QAAuB;MAAC,CADlE,EAEKH,IAFL,CAEU,UAACR,IAAD,EAAU;QAAC,KAAI,CAACE,SAAL,GAAe,KAAf;QAAqB,KAAI,CAACD,OAAL,GAAaD,IAAb;MAAmB,CAF7D,EAGKY,KAHL,CAGW,UAACT,KAAD,EAAS;QACdU,OAAO,CAACC,GAAR,CAAYX,KAAZ;QACF,KAAI,CAACA,KAAL,GAAW,6DAAX;MAEC,CAPL;IASF;EAbM;AAfK,CAAf","names":["SurveyResult","components","data","results","isLoading","error","mounted","getData","methods","fetch","then","response","ok","json","catch","console","log"],"sourceRoot":"","sources":["/Users/fasbhim/Documents/2022/work/2022_studyMaterial/vue-js/vue-js/doc/downloads/12.sendingHttpRequest/http-01-starting-setup/src/components/survey/UserExperiences.vue"],"sourcesContent":["<template>\n  <section>\n    <base-card>\n      <h2>Submitted Experiences</h2>\n      <div>\n        <base-button @click=\"getData\">Load Submitted Experiences</base-button>\n      </div>\n      <p v-if=\"error\">{{error}}</p>\n      <p v-else-if=\"isLoading\">Loading....</p>\n      <p v-else-if=\"!isLoading && (!results || results.length===0)\" >No stored Experience found</p>\n\n      <ul v-else-if=\"!isLoading && results  \">\n        <survey-result\n          v-for=\"result in results\"\n          :key=\"result.id\"\n          :name=\"result.name\"\n          :rating=\"result.rating\"\n        ></survey-result>\n      </ul>\n    </base-card>\n  </section>\n</template>\n\n<script>\nimport SurveyResult from './SurveyResult.vue';\n\nexport default {\n  // props: ['results'],\n  components: {\n    SurveyResult,\n  },\n  data(){\n    return {\n      results:[],\n      isLoading: false,\n      error:null\n    }\n  },\n  mounted() {\n    this.getData();\n  },\n  methods:{\n    getData(){\n      this.isLoading=true\n      this.error=null\n      fetch('https://vue-demo-a2598-default-rtdb.firebaseio.com/surveys.json')\n          .then((response) =>{ if(response.ok){ return response.json()}})\n          .then((data) => {this.isLoading=false;this.results=data;})\n          .catch((error)=>{\n            console.log(error)\n          this.error='Failed to fetch data. Check the server connection please!!!'\n\n          });\n\n    }\n  }\n};\n</script>\n\n<style scoped>\nul {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n</style>"]}]}